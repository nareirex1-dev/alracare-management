{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin",
      "dest": "/frontend/admin-site.html"
    },
    {
      "src": "/admin-site.html",
      "dest": "/frontend/admin-site.html"
    },
    {
      "src": "/public-site.html",
      "dest": "/frontend/public-site.html"
    },
    {
      "src": "/index.html",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/images/(.*)",
      "dest": "/frontend/images/$1"
    },
    {
      "src": "/(.*\\.css)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
